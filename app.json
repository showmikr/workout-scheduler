{
  "expo": {
    "name": "workout-scheduler",
    "slug": "workout-scheduler",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "868bf7f8-5f02-4f3b-bee4-62f5d1a7927f"
      }
    },
    "owner": "showmikr",
    "plugins": ["expo-router"],
    "scheme": "workoutscheduler",
    "experiments": {
      "typedRoutes": true
    }
  }
}
